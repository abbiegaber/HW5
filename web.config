<configuration>
    <connectionStrings>
        <add name="cs_AnimalShelter" connectionString="Data Source=.\SQLEXPRESS;AttachDbFilename=|DataDirectory|\AnimalShelterDatabase.mdf;Integrated Security=True;User Instance=True"
